{
  "instrument_info": {
    "name": "MoTe2_QAHE_Device",
    "vendor": "Ruihao,Jiaqi",
    "model": "MoTe2_QAHE_v1",
    "description": "MoTe2 Quantum Anomalous Hall Effect device with experiments",
    "firmware": "1.0.0"
  },
  "data_file": {
    "path": "processed_data.mat",
    "format": "matlab",
    "interpolation_method": "linear",
    "bounds_error": false,
    "fill_value": null
  },
  "parameters": {
    "independent": [
      {
        "name": "Vtg",
        "label": "Top Gate Voltage",
        "unit": "V",
        "settable": true,
        "gettable": true,
        "initial_value": 0.0,
        "data_key": "Vtg",
        "alias": "top_gate",
        "monitor": true,
        "description": "Top gate voltage control parameter",
        "validator": {
          "type": "numbers",
          "min": -4.5,
          "max": 0.975
        }
      },
      {
        "name": "Vbg",
        "label": "Back Gate Voltage", 
        "unit": "V",
        "settable": true,
        "gettable": true,
        "initial_value": 0.0,
        "data_key": "Vbg",
        "alias": "back_gate",
        "monitor": true,
        "description": "Back gate voltage control parameter",
        "validator": {
          "type": "numbers",
          "min": -12.485,
          "max": 1.0
        }
      }
    ],
    "dependent": [
      {
        "name": "lockin_xx",
        "label": "Longitudinal Resistance",
        "unit": "V",
        "settable": false,
        "gettable": true,
        "data_key": "lockin_xx",
        "alias": "resistance_xx",
        "monitor": true,
        "description": "Lock-in amplifier XX measurement (longitudinal resistance)",
        "depends_on": ["Vbg", "Vtg"]
      },
      {
        "name": "lockin_xy", 
        "label": "Hall Voltage",
        "unit": "V",
        "settable": false,
        "gettable": true,
        "data_key": "lockin_xy",
        "alias": "hall_voltage",
        "monitor": true,
        "description": "Lock-in amplifier XY measurement (Hall voltage)",
        "depends_on": ["Vbg", "Vtg"]
      },
      {
        "name": "lockin_i",
        "label": "Source Current",
        "unit": "A", 
        "settable": false,
        "gettable": true,
        "data_key": "lockin_i",
        "alias": "source_current",
        "monitor": true,
        "description": "Lock-in current measurement",
        "depends_on": ["Vbg", "Vtg"]
      }
    ]
  },
  "metadata": {
    "measurement_conditions": {
      "temperature": "Variable",
      "magnetic_field": "8 T", 
      "sample_info": "MoTe2 thin film on substrate"
    },
    "data_shape": {
      "vtg_points": 220,
      "vbg_points": 900,
      "total_points": 198000
    },
    "creation_date": "2025-09-06",
    "version": "1.0"
  }
}